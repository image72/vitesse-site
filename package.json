{
  "name": "vitesse-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.17.1",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "devtools": "vue-devtools",
    "lint:eslint": "eslint --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\"",
    "lint:prettier": "prettier --list-different --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
    "@unocss/preset-tagify": "^0.49.2",
    "@unocss/reset": "^0.47.6",
    "@vueuse/head": "^1.0.22",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@iconify-json/carbon": "^1.1.11",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue-macros/volar": "^0.5.4",
    "@vue/devtools": "^6.4.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vueuse/components": "^9.10.0",
    "@vueuse/core": "^9.6.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.0.2",
    "eslint": "^8.29.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "https-localhost": "^4.7.1",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.7.2",
    "pnpm": "^7.18.1",
    "prettier": "^2.8.2",
    "shiki": "^0.11.1",
    "taze": "^0.8.4",
    "typescript": "^4.9.4",
    "unocss": "^0.49.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-macros": "^1.0.3",
    "vite": "^4.0.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.7.10",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-pwa": "^0.14.0",
    "vite-plugin-vue-component-preview": "^0.3.3",
    "vite-plugin-vue-inspector": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vite-ssg": "^0.22.1",
    "vite-ssg-sitemap": "^0.4.3",
    "vitest": "^0.25.7",
    "vue-tsc": "^1.0.13"
