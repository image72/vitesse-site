{
  "name": "vitesse-site",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@7.17.1",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite -m development --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "devtools": "vue-devtools",
    "lint:eslint": "eslint --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\"",
    "lint:prettier": "prettier --list-different --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
    "@unocss/preset-tagify": "^0.51.12",
    "@unocss/reset": "^0.51.12",
    "@vueuse/head": "^1.1.26",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-demi": "^0.14.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@iconify-json/carbon": "^1.1.16",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue-macros/volar": "^0.9.8",
    "@vue/devtools": "^6.5.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "eslint": "^8.40.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^4.2.1",
    "https-localhost": "^4.7.1",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^7.32.4",
